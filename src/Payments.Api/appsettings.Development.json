{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "Payments": "Debug"
    }
  },
  "Payout": {
    "DefaultProvider": "Mesta",
    "EnableFailover": true,
    "QuoteValidityMinutes": 5,
    "MaxRetryAttempts": 3,
    "Mesta": {
      "BaseUrl": "https://api.stg.mesta.xyz/v1",
      "ApiKey": "test_api_key",
      "MerchantId": "test_merchant_id",
      "WebhookSecret": "test_webhook_secret",
      "TimeoutSeconds": 30,
      "RetryAttempts": 3,
      "Enabled": true
    },
    "Borderless": {
      "BaseUrl": "https://api.sandbox.borderless.xyz/v1",
      "ApiKey": "test_api_key",
      "ApiSecret": "test_api_secret",
      "ClientId": "test_client_id",
      "WebhookSecret": "test_webhook_secret",
      "TimeoutSeconds": 30,
      "RetryAttempts": 3,
      "Enabled": true
    }
  }
}
